---
import Heading from "../Typography/Heading.astro";
import WorkPlace from "../WorkPlace.astro";

const { workBackground } = Astro.props;
---

<div>
  <Heading>work experience</Heading>
  {
    workBackground.map(
      (el: {
        position: string;
        workPlace: string;
        start: string;
        end?: string;
        content: string[];
      }) => <WorkPlace {...el} />
    )
  }
</div>
